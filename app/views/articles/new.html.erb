<div class="container">
<h1>Create a new Article</h1>

<%= form_for(@article) do |f| %>
  <%= f.label :Title %>
  <%= f.text_field :title %>
  <%= f.label :Content %>
  <%= f.text_field :content%>
  <%= f.submit  class: "btn btn-success"%>
<% end %>
<p><%= link_to "home", articles_path, class: "btn btn-secondary" %>
</div>

